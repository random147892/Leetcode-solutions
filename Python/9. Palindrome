// Runtime 82ms
class Solution:
    def isPalindrome(self, x: int) -> bool:
        for digit in str(x):
            for digit2 in str(x)[::-1]:
                if digit == digit2:
                    return True
                else:
                    return False
//Runtim 62ms
class Solution2:
    def isPalindrome(self, x: int) -> bool:
        if str(x) == str(x)[::-1]:
            return True
        else:
            return False
